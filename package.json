{
  "name": "boilerplate-lambda",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "nodemon --exec ts-node src/main-dev.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "keywords": [],
  "description": "",
  "devDependencies": {
    "@aws-sdk/client-sqs": "^3.665.0",
    "@types/async": "^3.2.24",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "async": "^3.2.6",
    "aws-sdk": "^2.1691.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "sqs-consumer": "^11.1.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-transform-paths": "^3.5.1",
    "@jeordanecarlosbatista/test": "^1.0.6"
  },
  "dependencies": {
    "@jeordanecarlosbatista/jcb-aws-sqs": "^1.0.20",
    "dotenv": "^16.4.5",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup-env.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "coveragePathIgnorePatterns": [],
    "moduleNameMapper": {
      "^@test/(.*)$": "<rootDir>/test/$1",
      "^@app/(.*)$": "<rootDir>/src/$1"
    },
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testEnvironment": "node"
  }
}
